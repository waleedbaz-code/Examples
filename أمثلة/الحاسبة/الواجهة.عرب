مهمة تنفيذ1(القيمة){
متغير النتيجة=الصفحة.العنصر("النتيجة")؛
النتيجة.النص=القيمة؛
}

مهمة نقر(المدخل){
متغير النتيجة=الصفحة.العنصر("النتيجة")؛
متغير م=متغيرات_المهمة()؛
   م.اضافة(المدخل)؛
    م.اضافة(النتيجة.النص)؛
 
  مهمةخدمة("عملية",م.المحتوى(),تنفيذ1) ؛


//النتيجة.النص=( مهمةخدمة("عملية",م.المحتوى()) )؛
//النتيجة.النص=النتيجة.النص+ق؛
}
